{
    "extends": "@tsconfig/svelte/tsconfig.json",
    "include": ["./**/*",],
    "exclude": ["../node_modules/*"],
    "compilerOptions": {
        "strict": true, 
        "types": ["node", "svelte"],
        "baseUrl": ".",
        "paths": {
            "@utils/*": [
                "webviews/utils/*"
            ],
            "@types/*": [
                "webviews/types/*"
            ],
            "@components/*": [
                "webviews/components/*"
            ]
        }
    }

}